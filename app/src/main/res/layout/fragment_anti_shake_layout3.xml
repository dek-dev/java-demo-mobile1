<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:src="@mipmap/anti_shake_pro_compared_bg" />

    <com.oppo.marketdemo.custom.TextureVideoView
        android:id="@+id/texture_video_view3"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="invisible"/>




    <com.oppo.marketdemo.custom.textview.TypefaceTextView
        style="@style/compare_point_guadi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/anti_shake_pro_guadi"
        android:layout_marginStart="40dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:typeface_style="mid" />


    <com.oppo.marketdemo.custom.textview.TypefaceTextView
        style="@style/compare_point_other"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="200dp"
        android:text="@string/anti_shake_pro_other"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:typeface_style="mid" />
    <ImageView
        android:id="@+id/iv_camera3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="400dp"
        app:layout_constraintTop_toTopOf="parent"
        android:src="@mipmap/ic_camera" />
</androidx.constraintlayout.widget.ConstraintLayout>
